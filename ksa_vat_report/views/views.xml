<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.view.inherited.vat.report</field>
            <field name="model">res.config.settings</field>
<!--            <field name="priority" eval="31"/>-->
             <field name="inherit_id" ref="account.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//block[@id='analytic']" position="after">
                        <h2>VAT Report</h2>
                     <div class="row mt16 o_settings_container" id="vat_report">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane" id="snailmail_settings">
                            <label for="vat_report_previous_start_date" />
                            <div class="text-muted">

                            </div>
                            <field name="vat_report_previous_start_date" on_change="1"/>

                        </div>
                    </div>
                     </div>
                </xpath>
            </field>
        </record>


</odoo>